<?php
// Postavljanje kolačića
$vrijemeTrajanja = time() + (86400 * 30); // 86400 sekundi = 1 dan, kolačić će trajati 30 dana
setcookie("korisnik", "Aleksandra Blažeković", $vrijemeTrajanja, "/"); // "/" znači da je kolačić dostupan na cijelom sajtu

echo "Kolačić postavljen! Posjetite index.php za provjeru kolačića.";
?>

